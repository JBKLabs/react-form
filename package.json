{
  "name": "@jbknowledge/react-form",
  "version": "0.2.0",
  "description": "Form utilities for react web and native projects",
  "main": "dist/index.js",
  "react-native": "native/dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "lint": "eslint src/* && prettier src/*",
    "lint:fix": "eslint src/* --fix && prettier src/* --write"
  },
  "keywords": [
    "jbknowledge",
    "react",
    "form"
  ],
  "homepage": "https://github.com/JBKLabs/react-form",
  "bugs": {
    "url": "https://github.com/JBKLabs/react-form/issues"
  },
  "license": "Apache-2.0",
  "authors": [
    "Brian Maule (https://github.com/mauleb)"
  ],
  "files": [
    "dist",
    "native"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JBKLabs/react-form.git"
  },
  "peerDependencies": {
    "react": "^16.8.3"
  },
  "optionalDependencies": {
    "react-native": "^0.62.3"
  },
  "devDependencies": {
    "@babel/core": "7.5.4",
    "@babel/plugin-external-helpers": "7.2.0",
    "@babel/plugin-proposal-class-properties": "7.5.0",
    "@babel/preset-react": "7.0.0",
    "@jbknowledge/eslint-config": "0.0.12",
    "eslint": "6.4.0",
    "prettier": "1.18.2",
    "rollup": "1.17.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.0.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-sourcemaps": "0.4.2"
  },
  "dependencies": {
    "minimatch": "3.0.4",
    "randomstring": "1.1.5"
  },
  "eslintConfig": {
    "extends": [
      "@jbknowledge"
    ],
    "rules": {
      "indent": [
        "error",
        2
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "tabWidth": 2
  }
}